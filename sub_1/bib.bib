@article{bivolarova_comparison_2017,
	title        = {A comparison between tracer gas and aerosol particles distribution indoors: {The} impact of ventilation rate, interaction of airflows, and presence of objects},
	shorttitle   = {A comparison between tracer gas and aerosol particles distribution indoors},
	author       = {Bivolarova, M. and Ondr\'{a}\v{c}ek, J. and Melikov, A. and \v{Z}d\'{\i}mal, V.},
	year         = 2017,
	journal      = {Indoor Air},
	volume       = 27,
	number       = 6,
	pages        = {1201--1212},
	doi          = {https://doi.org/10.1111/ina.12388},
	issn         = {1600-0668},
	urldate      = {2021-03-30},
	copyright    = {\textcopyright{} 2017 John Wiley \& Sons A/S. Published by John Wiley \& Sons Ltd},
	note         = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ina.12388},
	abstract     = {The study investigated the separate and combined effects of ventilation rate, free convection flow produced by a thermal manikin, and the presence of objects on the distribution of tracer gas and particles in indoor air. The concentration of aerosol particles and tracer gas was measured in a test room with mixing ventilation. Three layouts were arranged: an empty room, an office room with an occupant sitting in front of a table, and a single-bed hospital room. The room occupant was simulated by a thermal manikin. Monodisperse particles of three sizes (0.07, 0.7, and 3.5 \ensuremath{\mu}m) and nitrous oxide tracer gas were generated simultaneously at the same location in the room. The particles and gas concentrations were measured in the bulk room air, in the breathing zone of the manikin, and in the exhaust air. Within the breathing zone of the sitting occupant, the tracer gas emerged as reliable predictor for the exposure to all different-sized test particles. A change in the ventilation rate did not affect the difference in concentration distribution between tracer gas and larger particle sizes. Increasing the room surface area did not influence the similarity in the dispersion of the aerosol particles and the tracer gas.},
	keywords     = {exposure, particles, room air distribution, thermal manikin, tracer gas, transport behavior},
	language     = {en}
}

