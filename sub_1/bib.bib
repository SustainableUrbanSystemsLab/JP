@article{ali_data-driven_2019,
	title        = {A data-driven approach for multi-scale building archetypes development},
	author       = {Ali, Usman and Shamsi, Mohammad Haris and Hoare, Cathal and Mangina, Eleni and O’Donnell, James},
	year         = 2019,
	month        = nov,
