@article{coceal_mean_2006,
	title        = {Mean flow and turbulence statistics over groups of urban-like cubical obstacles},
	author       = {Coceal, O. and Thomas, T. G. and Castro, I. P. and Belcher, S. E.},
	year         = 2006,
	journal      = {Boundary-Layer Meteorology},
	volume       = 121,
