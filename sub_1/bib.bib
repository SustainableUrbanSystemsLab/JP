@article{iaccarino_reynolds_2003,
	title        = {Reynolds averaged simulation of unsteady separated flow},
	author       = {Iaccarino, G. and Ooi, A. and Durbin, P. A. and Behnia, M.},
	year         = 2003,
	journal      = {International Journal of Heat and Fluid Flow},
	volume       = 24,
	number       = 2,
	pages        = {147--156},
	doi          = {10.1016/S0142-727X(02)00210-2},
	isbn         = {0142-727X},
	issn         = {0142727X},
	note         = {ISBN: 0142-727X},
	abstract     = {The accuracy of Reynolds averaged Navier-Stokes (RANS) turbulence models in predicting complex flows with separation is examined. The unsteady flow around square cylinder and over a wall-mounted cube are simulated and compared with experimental data. For the cube case, none of the previously published numerical predictions obtained by steady-state RANS produced a good match with experimental data. However, evidence exists that coherent vortex shedding occurs in this flow. Its presence demands unsteady RANS computation because the flow is not statistically stationary. The present study demonstrates that unsteady RANS does indeed predict periodic shedding, and leads to much better concurrence with available experimental data than has been achieved with steady computation. \textcopyright{} 2002 Elsevier Science Inc. All rights reserved.}
}

