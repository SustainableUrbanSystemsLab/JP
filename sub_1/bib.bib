@article{rosbach_ventilation_2013,
	title        = {A ventilation intervention study in classrooms to improve indoor air quality: the {FRESH} study.},
	author       = {Rosbach, Jeannette T M and Vonk, Machiel and Duijm, Frans and van Ginkel, Jan T and Gehring, Ulrike and Brunekreef, Bert},
	year         = 2013,
	journal      = {Environmental health : a global access science source},
	volume       = 12,
	pages        = 110,
	doi          = {10.1186/1476-069X-12-110},
	isbn         = {1476-069X},
	issn         = {1476-069X},
	note         = {ISBN: 1476-069X},
	abstract     = {BACKGROUND: Classroom ventilation rates often do not meet building standards, although it is considered to be important to improve indoor air quality. Poor indoor air quality is thought to influence both children's health and performance. Poor ventilation in The Netherlands most often occurs in the heating season. To improve classroom ventilation a tailor made mechanical ventilation device was developed to improve outdoor air supply. This paper studies the effect of this intervention.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$nMETHODS: The FRESH study (Forced-ventilation Related Environmental School Health) was designed to investigate the effect of a CO2 controlled mechanical ventilation intervention on classroom CO2 levels using a longitudinal cross-over design. Target CO2 concentrations were 800 and 1200 parts per million (ppm), respectively. The study included 18 classrooms from 17 schools from the north-eastern part of The Netherlands, 12 experimental classrooms and 6 control classrooms. Data on indoor levels of CO2, temperature and relative humidity were collected during three consecutive weeks per school during the heating seasons of 2010-2012. Associations between the intervention and weekly average indoor CO2 levels, classroom temperature and relative humidity were assessed by means of mixed models with random school-effects.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$nRESULTS: At baseline, mean CO2 concentration for all schools was 1335 ppm (range: 763-2000 ppm). The intervention was able to significantly decrease CO2 levels in the intervention classrooms (F (2,10) = 17.59, p {\textbackslash}textless 0.001), with a mean decrease of 491 ppm. With the target set at 800 ppm, mean CO2 was 841 ppm (range: 743-925 ppm); with the target set at 1200 ppm, mean CO2 was 975 ppm (range: 887-1077 ppm).\${\textbackslash}backslash\$n\${\textbackslash}backslash\$nCONCLUSIONS: Although the device was not capable of precisely achieving the two predefined levels of CO2, our study showed that classroom CO2 levels can be reduced by intervening on classroom ventilation using a CO2 controlled mechanical ventilation system.},
	keywords     = {Humans, Ventilation, Carbon Dioxide, Air Pollutants, Schools, Air Pollutants: analysis, Air Pollution, Carbon Dioxide: analysis, Child, Cross-Over Studies, Indoor, Indoor: analysis, Intervention Studies, Longitudinal Studies, Netherlands, Seasons, Ventilation: methods},
	pmid         = 24345039
}

