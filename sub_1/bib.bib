@article{veldhuizen_multiobjective_2000,
	title        = {Multiobjective {Evolutionary} {Algorithms}: {Analyzing} the {State}-of-the-{Art}},
	shorttitle   = {Multiobjective {Evolutionary} {Algorithms}},
	author       = {Veldhuizen, David A. Van and Lamont, Gary B.},
	year         = 2000,
	month        = jun,
	journal      = {Evolutionary Computation},
	volume       = 8,
	number       = 2,
	pages        = {125--147},
	doi          = {10.1162/106365600568158},
	issn         = {1063-6560, 1530-9304},
	urldate      = {2020-11-16},
	abstract     = {Solving optimization problems with multiple (often con icting) objectives is, generally, a very dif cult goal. Evolutionary algorithms (EAs) were initially extended and applied during the mid-eighties in an attempt to stochastically solve problems of this generic class. During the past decade, a variety of multiobjective EA (MOEA) techniques have been proposed and applied to many scienti c and engineering applications. Our discussion's intent is to rigorously de ne multiobjective optimization problems and certain related concepts, present an MOEA classi cation scheme, and evaluate the variety of contemporary MOEAs. Current MOEA theoretical developments are evaluated; speci c topics addressed include tness functions, Pareto ranking, niching, tness sharing, mating restriction, and secondary populations. Since the development and application of MOEAs is a dynamic and rapidly growing activity, we focus on key analytical insights based upon critical MOEA evaluation of current research and applications. Recommended MOEA designs are presented, along with conclusions and recommendations for future work.},
	language     = {en}
}

@article{verderio_junior_numerical_2018,
	title        = {Numerical validation of {viewFactor} and {FVDOM} radiation models of {OpenFOAM}\textregistered{} and application in the study of food furnaces},
	author       = {Verd\'{e}rio J\'{u}nior, S\'{\i}lvio Aparecido and Scalon, Vicente Luiz and Avallone, Elson and Mioralli, Paulo C\'{e}sar},
